   "source": [
    "from datetime import timedelta\n",
    "\n",
    "import pandas as pd\n",
    "\n",
    "\n",
    "def gap_splitter(timedelta: pd.Series, duration: str | timedelta) -> pd.Series:\n",
    "    duration = pd.Timedelta(duration)\n",
    "\n",
    "    diff = timedelta > duration\n",
    "    gaps = diff.cumsum()\n",
    "    gaps.name = 'gaps'\n",
    "\n",
    "    return gaps"
   ]
  }
